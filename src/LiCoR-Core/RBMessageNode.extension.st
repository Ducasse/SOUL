Extension { #name : #RBMessageNode }

{ #category : #'*LiCoR-Core' }
RBMessageNode >> soulChildren [
	
	^OrderedCollection
		with: self receiver
		with: self selector
		with: self arguments
]

{ #category : #'*LiCoR-Core' }
RBMessageNode >> soulChildren: children [
	
	self receiver: (children at: 1).
	self arguments: (children at: 3).	"if the arguments are not set before than the selecter an error will be found"

	self selector: (children at: 2)
]

{ #category : #'*LiCoR-Core' }
RBMessageNode >> unifyWith: aTerm inEnv: anEnv myIndex: myIndex hisIndex: hisIndex inSource: aBool [
	"GENERATED BY UNIFICATION EDITOR - DO NOT EDIT MANUALLY !!"

	
	^aTerm
		unifyWithRBMessageNode: self
		inEnv: anEnv
		myIndex: hisIndex
		hisIndex: myIndex
		inSource: aBool not
]

{ #category : #'*LiCoR-Core' }
RBMessageNode >> unifyWithRBMessageNode: aTerm inEnv: anEnv myIndex: myIndex hisIndex: hisIndex inSource: aBool [
	
	^self = aTerm
]
