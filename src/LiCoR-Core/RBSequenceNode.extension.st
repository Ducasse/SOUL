Extension { #name : #RBSequenceNode }

{ #category : #'*LiCoR-Core' }
RBSequenceNode >> soulChildren [
	
	^OrderedCollection
		with: self temporaries
		with: self statements
]

{ #category : #'*LiCoR-Core' }
RBSequenceNode >> soulChildren: children [
	
	self temporaries: (children at: 1).
	self statements: (children at: 2)
]

{ #category : #'*LiCoR-Core' }
RBSequenceNode >> unifyWith: aTerm inEnv: anEnv myIndex: myIndex hisIndex: hisIndex inSource: aBool [
	"GENERATED BY UNIFICATION EDITOR - DO NOT EDIT MANUALLY !!"

	
	^aTerm
		unifyWithRBSequenceNode: self
		inEnv: anEnv
		myIndex: hisIndex
		hisIndex: myIndex
		inSource: aBool not
]

{ #category : #'*LiCoR-Core' }
RBSequenceNode >> unifyWithRBSequenceNode: aTerm inEnv: anEnv myIndex: myIndex hisIndex: hisIndex inSource: aBool [
	
	^self = aTerm
]
